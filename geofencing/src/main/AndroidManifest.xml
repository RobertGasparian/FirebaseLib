<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.geofencing">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true">

        <service android:name=".services.HandleJsonService" />
        <receiver android:name=".receivers.GeoFenceReceiver">

            <intent-filter>
                <action android:name="com.example.geofencing.receivers.ENTER_INTENT">

                </action>
                <action android:name="com.example.geofencing.receivers.EXIT_INTENT">

                </action>
            </intent-filter>

        </receiver>

    </application>

</manifest>
